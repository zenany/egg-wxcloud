{
  "name": "egg-wxcloud",
  "version": "1.0.0",
  "description": "egg for wechat minapp cloud",
  "private": true,
  "egg": {
    "typescript": true,
    "declarations": true
  },
  "dependencies": {
    "egg": "^3.17.5",
    "egg-scripts": "2"
  },
  "devDependencies": {
    "@eggjs/tsconfig": "^1.3.3",
    "@eggjs/bin": "^7.3.1",
    "@types/cheerio": "^0.22.35",
    "@types/mocha": "^10.0.10",
    "typescript": "^5.8.2",
    "egg-mock": "^5.15.2",
    "eslint": "8",
    "eslint-config-egg": "13"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "egg-scripts start --port=80 --title=egg-server-egg-wxcloud",
    "stop": "egg-scripts stop --title=egg-server-egg-wxcloud",
    "dev": "egg-bin dev",
    "tsc": "tsc -p tsconfig.json",
    "clean": "tsc -b --clean",
    "test": "npm run lint -- --fix && npm run test:local",
    "test:local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && npm run cov"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "",
  "license": "MIT"
}
